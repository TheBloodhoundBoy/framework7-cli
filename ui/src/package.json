{
  "name": "framework7-cli-ui",
  "private": true,
  "version": "1.0.0",
  "description": "Framework7 CLI UI",
  "repository": "",
  "license": "MIT",
  "framework7": {
    "type": [
      "web"
    ],
    "name": "Framework7 CLI",
    "framework": "vue",
    "template": "single-view",
    "customColor": false,
    "bundler": "webpack"
  },
  "scripts": {
    "build-prod": "cross-env NODE_ENV=production cross-env BUILD=true node ./build/build.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.js",
    "dev-watch": "cross-env NODE_ENV=development cross-env BUILD=true webpack --progress --config ./build/webpack.config.js --watch",
    "start": "npm run dev"
  },
  "browserslist": [
    "Android >= 5",
    "IOS >= 9.3",
    "Edge >= 15",
    "Safari >= 9.1",
    "Chrome >= 49",
    "Firefox >= 31",
    "Samsung >= 5"
  ],
  "dependencies": {
    "dom7": "^3.0.0",
    "framework7": "^6.0.22",
    "framework7-icons": "^3.0.0",
    "framework7-vue": "^6.0.22",
    "vue": "^3.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@vue/compiler-sfc": "^3.0.4",
    "babel-loader": "^8.0.6",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.0",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^4.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.1.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "vue-loader": "^16.1.2",
    "vue-style-loader": "^4.1.2",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-livereload-plugin": "^2.2.0"
  }
}
